.row.hidden-print
  .col-md-12.clearfix
    = link_to "Print", "javascript:void(0)", class: "print-page pull-right btn btn-primary"
    %br
    %br

#print-list
  - @donors.each do |donor|
    .panel.panel-default
      .panel-body
        %p.lead= donor.full_name
        %p.address= raw donor.printable_address
        %p= donor.contact_number
        %p= donor.email
